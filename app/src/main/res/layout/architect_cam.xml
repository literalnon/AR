<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:id="@+id/mainLayout"

	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<!--<com.wikitude.architect.ArchitectView
		android:id="@+id/architectView"
		android:layout_width="0dp"
		android:layout_height="0dp"
		android:keepScreenOn="true"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />-->

	<FrameLayout
		android:id="@+id/architectViewLayout"
		android:layout_width="0dp"
		android:layout_height="0dp"
		android:keepScreenOn="true"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<FrameLayout
		android:id="@+id/zoomView"
		android:layout_width="0dp"
		android:layout_height="0dp"
		android:background="@android:color/transparent"
		app:layout_constraintBottom_toTopOf="@+id/foto_button"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<androidx.appcompat.widget.AppCompatImageView
		android:id="@+id/foto_button"
		android:layout_width="64dp"
		android:layout_height="64dp"
		android:layout_alignParentBottom="true"
		android:layout_centerHorizontal="true"
		android:layout_marginStart="16dp"
		android:layout_marginEnd="16dp"
		android:layout_marginBottom="32dp"
		android:background="@drawable/camera_150"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintEnd_toStartOf="@+id/fab"
		app:layout_constraintHorizontal_bias="0.5"
		app:layout_constraintStart_toEndOf="@+id/videoButton" />

	<androidx.appcompat.widget.AppCompatImageView
		android:id="@+id/videoButton"
		android:layout_width="64dp"
		android:layout_height="64dp"
		android:layout_marginEnd="16dp"
		app:layout_constraintBottom_toBottomOf="@+id/foto_button"
		app:layout_constraintEnd_toStartOf="@+id/foto_button"
		app:layout_constraintHorizontal_bias="0.5"
		app:layout_constraintHorizontal_chainStyle="packed"
		app:layout_constraintStart_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="@+id/foto_button"
		app:srcCompat="@drawable/video_150" />

	<androidx.appcompat.widget.AppCompatImageView
		android:id="@+id/rUp"
		android:layout_width="50dp"
		android:layout_height="70dp"
		android:layout_alignParentRight="true"
		android:alpha="0.4"
		android:background="@drawable/pravy_verkhniy"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintTop_toTopOf="parent" />


	<androidx.appcompat.widget.AppCompatImageView
		android:id="@+id/lUp"
		android:layout_width="50dp"
		android:layout_height="70dp"
		android:alpha="0.4"
		android:background="@drawable/levy_verkhniy"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<androidx.appcompat.widget.AppCompatImageView
		android:id="@+id/lDown"
		android:layout_width="50dp"
		android:layout_height="70dp"
		android:layout_alignParentBottom="true"
		android:alpha="0.4"
		android:background="@drawable/levy_nizhniy"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintStart_toStartOf="parent" />

	<androidx.appcompat.widget.AppCompatImageView
		android:id="@+id/rDown"
		android:layout_width="50dp"
		android:layout_height="70dp"
		android:layout_alignParentRight="true"
		android:layout_alignParentBottom="true"
		android:alpha="0.4"
		android:background="@drawable/pravy_nizhniy"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent" />

	<androidx.appcompat.widget.AppCompatTextView
		android:id="@+id/tvTime"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="32dp"
		android:layout_marginBottom="48dp"
		android:textColor="@android:color/white"
		android:textSize="20sp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent" />

	<com.getbase.floatingactionbutton.FloatingActionsMenu
		android:id="@+id/fab"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="8dp"
		android:layout_marginBottom="24dp"
		android:clickable="true"
		app:fab_addButtonColorNormal="@color/colorAccent"
		app:fab_addButtonPlusIconColor="@android:color/black"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.5"
		app:layout_constraintStart_toEndOf="@+id/foto_button"
		>

		<com.getbase.floatingactionbutton.FloatingActionButton
			android:id="@+id/webInfoFab"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			app:fab_colorNormal="@android:color/white"
			app:fab_colorPressed="@android:color/white"
			app:fab_icon="@drawable/web_150" />

		<com.getbase.floatingactionbutton.FloatingActionButton
			android:id="@+id/informationFab"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			app:fab_colorNormal="@android:color/white"
			app:fab_colorPressed="@android:color/white"
			app:fab_icon="@drawable/info_150" />

		<com.getbase.floatingactionbutton.FloatingActionButton
			android:id="@+id/geoArSwitcher"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			app:fab_colorNormal="@android:color/white"
			app:fab_colorPressed="@android:color/white"
			app:fab_icon="@drawable/geo_ar_150" />

		<com.getbase.floatingactionbutton.FloatingActionButton
			android:id="@+id/flashSwitcher"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:background="@android:color/white"
			app:fab_colorNormal="@android:color/white"
			app:fab_colorPressed="@color/colorAccent"
			app:fab_icon="@drawable/flash_150" />

		<com.getbase.floatingactionbutton.FloatingActionButton
			android:id="@+id/switchCameraFab"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			app:fab_colorNormal="@android:color/white"
			app:fab_colorPressed="@android:color/white"
			app:fab_icon="@drawable/cameraflip_150" />

	</com.getbase.floatingactionbutton.FloatingActionsMenu>

</androidx.constraintlayout.widget.ConstraintLayout>

